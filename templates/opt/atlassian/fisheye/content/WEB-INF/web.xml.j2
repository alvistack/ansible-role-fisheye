        <servlet-class>com.cenqua.fisheye.web.ActionDispatcherServlet</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>
    <servlet>
        <servlet-name>ViewCvsRemapperServlet</servlet-name>
        <servlet-class>com.cenqua.fisheye.web.ViewCvsRemapperServlet</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>
    <servlet>
        <servlet-name>ActivityStreamServlet</servlet-name>
        <servlet-class>com.cenqua.fisheye.web.ActivityStreamServlet</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>
    <servlet>
        <servlet-name>RestApiServlet</servlet-name>
        <servlet-class>com.cenqua.fisheye.api.RestApiServlet</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>
    <servlet>
        <servlet-name>XmlRpcApiServlet</servlet-name>
        <servlet-class>com.cenqua.fisheye.api.XmlRpcApiServlet</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>
    <servlet>
        <servlet-name>webwork</servlet-name>
        <servlet-class>com.opensymphony.webwork.dispatcher.ServletDispatcher</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>file-server</servlet-name>
        <servlet-class>com.atlassian.fisheye.plugin.webresource.FisheyeFileServerServlet</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>

    <servlet>
        <servlet-name>gwtServlet</servlet-name>
        <servlet-class>com.atlassian.fecru.gwt.servlet.GwtRPCRoutingServlet</servlet-class>
    </servlet>

    <!-- repository management -->
    <servlet>
        <servlet-name>git-server</servlet-name>
        <servlet-class>com.atlassian.fisheye.repomgmt.servlet.GitServlet</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>error-servlet</servlet-name>
        <servlet-class>com.cenqua.fisheye.web.ErrorServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>git-server</servlet-name>
        <url-pattern>/git/*</url-pattern>
    </servlet-mapping>

    <!-- end repository management -->

    <servlet>
        <servlet-name>uploadServlet</servlet-name>
        <!-- This is the default servlet, it puts files in session -->
        <servlet-class>com.atlassian.fecru.gwt.servlet.GwtSshKeyUploadServlet</servlet-class>
        <init-param>
            <param-name>maxSize</param-name>
            <param-value>5</param-value>
        </init-param>
    </servlet>
    <servlet-mapping>
        <servlet-name>uploadServlet</servlet-name>
        <url-pattern>*.gupld</url-pattern>
    </servlet-mapping>

