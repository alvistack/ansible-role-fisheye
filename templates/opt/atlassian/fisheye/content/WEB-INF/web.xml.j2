<?xml version="1.0" encoding="ISO-8859-1"?>
<!-- {{ ansible_managed }} -->
<web-app
        xmlns="http://java.sun.com/xml/ns/javaee"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
        metadata-complete="true"
        version="3.0">
    <display-name>FishEye WebApp</display-name>

    <session-config>
        <cookie-config>
            <name>FESESSIONID</name>
            <http-only>true</http-only>
        </cookie-config>
        <tracking-mode>COOKIE</tracking-mode>
        <session-timeout>120</session-timeout>
    </session-config>

    <!-- Listeners -->
    <!-- load the Spring context from WEB-INF/applicationContext.xml -->
    <listener>
        <listener-class>
            com.cenqua.fisheye.web.FisheyeContextLoader
        </listener-class>
    </listener>


    <!-- Define Filters -->

    <!-- Plugin filters -->

    <filter>
        <filter-name>filter-plugin-dispatcher-after-encoding-request</filter-name>
        <filter-class>com.atlassian.fisheye.plugin.servlet.FisheyeServletFilterModuleContainerFilter</filter-class>
        <init-param>
            <param-name>location</param-name>
            <param-value>after-encoding</param-value>
        </init-param>
        <init-param>
            <param-name>dispatcher</param-name>
            <param-value>REQUEST</param-value>
        </init-param>
    </filter>

    <filter>
        <filter-name>filter-plugin-dispatcher-after-encoding-forward</filter-name>
        <filter-class>com.atlassian.fisheye.plugin.servlet.FisheyeServletFilterModuleContainerFilter</filter-class>
        <init-param>
            <param-name>location</param-name>
            <param-value>after-encoding</param-value>
        </init-param>
        <init-param>
            <param-name>dispatcher</param-name>
            <param-value>FORWARD</param-value>
        </init-param>
    </filter>

    <filter>
